<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.whir.webplatform.manage.po.WeixinMenuPO" table="SYS_CORP_SET_APP" lazy="false" schema="EZOFFICE">
        <id name="id" column="id" type="long">
             <generator class="native"/>
        </id>

		<property name="appname" column="APPNAME"/>
		<property name="appId" column="APPID"/>
		<property name="corpId" column="CORPID"/>
		<property name="corpsecret" column="CORPSECRET"/>
	</class>
</hibernate-mapping>
