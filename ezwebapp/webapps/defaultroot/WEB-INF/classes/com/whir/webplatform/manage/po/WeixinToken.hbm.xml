<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.whir.webplatform.manage.po.WeixinTokenPO" table="ezmobile_wxToken" lazy="false" schema="EZOFFICE">
        <id name="id" column="id" type="long">
             <generator class="native"/>
        </id>
		<property name="secret" column="corpsecret"/>
		<property name="token" column="wxToken"/>
		<property name="timeStamp" column="tokenTimeStamp"/>
	</class>
</hibernate-mapping>
